{
    "app": {
        "name": "iOS Weekly",
        "logoUrl": "didomiLogo",
        "privacyPolicyURL": "http://www.privacy.policy",
        "vendors": {
            "iab": {
                "all": false,
                "include": [8, 9, 10, 11, 12, 13, 14, 15],
                "exclude": []
            },
            "didomi": [
                "google",
                "amazon"
            ],
            "custom": [
                {
                    "id": "singlespot",
                    "name": "SingleSpot",
                    "purposeIds": [
                        "geo_ads",
                        "geo_marketing_studies"
                    ],
                    "policyUrl": "https://www.singlespot.com/privacy_policy",
                    "iabId": 435
                }
            ]
        },
        "gdprAppliesGlobally": true,
        "gdprAppliesWhenUnknown": false,
        "customPurposes": [
            {
                "id": "geo_ads",
                "name": {
                    "en": "Storage and use of geolocation data for targeted advertising",
                    "fr": "La conservation et l’accès aux informations de géolocalisation à des fins de publicité ciblée"
                },
                "description": {
                    "fr": "Notre partenaire géomarketing souhaite, pour le compte de ses clients (magasins de mode, magasins de voitures, hypermarchés, etc.), vous proposer des publicités personnalisées adaptées à vos centres d’intérêt et vous permettre de connaitre les offres disponibles autour de vous. Il est important de bien comprendre, que si vous décidez de refuser la collecte de vos données à des fins de publicité ciblée, cela ne signifie pas pour autant que vous ne verrez plus de publicité mais que celle-ci sera générique c’est à dire pas adaptée à vos centres d'intérêt."
                }
            },
            {
                "id": "geo_marketing_studies",
                "name": {
                    "en": "Storage and use of geolocation data for marketing studies",
                    "fr": "La conservation et l’accès aux informations de géolocalisation pour réaliser des études marketing"
                },
                "description": {
                    "fr": "Notre partenaire géomarketing souhaite pouvoir réaliser des études marketing pour le compte de ses clients en vue, en particulier, de les aider à avoir une meilleure compréhension du marché et de ses tendances et de les aider à analyser la performance de leurs magasins et celle de leurs concurrents. Ces rapports marketing ne contiendront que des données agrégées et anonymes."
                }
            }
        ]
    },
    "notice": {
        "enable": true,
        "position": "popup",
        "content": {
            "dismiss": {
                "fr": "dismiss fr",
                "en": "dismiss en"
            },
            "learnMore": {
                "fr": "learnMore fr",
                "en": "learnMore en"
            }
        }
    },
    "preferences": {
        "showWhenConsentIsMissing": false,
        "canCloseWhenConsentIsMissing": true,
        "content": {
            "textVendors": {
                "fr": "This is a custom text",
                "en": "This is a custom text"
            },
            "subTextVendors": {
                "fr": "subTextVendors - en",
                "en": "subTextVendors - fr"
            },
        }
    },
    "theme": {
        "color": "#000000",
        "linkColor": "#F9E5E2",
        "buttons": {
            "regularButtons": {
                "backgroundColor": "#777777",
                "textColor": "#999999",
                "borderColor": "#000000",
                "borderWidth": "3",
                "borderRadius": "10"
            },
            "highlightButtons": {
                "backgroundColor": "#123456",
                "textColor": "#A4A4A4",
                "borderWidth": "5",
                "borderColor": "#777777",
                "borderRadius": "20"
            }
        }
    },
    "texts": {
        "preferences.content.text_with_underscore_1": {
            "en": "Custom key 1 in English",
            "fr": "Clé personalisé 1 en Français",
        },
        "preferences.content.text_with_underscore_2": {
            "en": "Custom key 2 in English",
            "fr": "Clé personalisé 2 en Français",
        }
    },
    "languages": {
        "enabled": ["fr", "hr", "en"]
    }
}
